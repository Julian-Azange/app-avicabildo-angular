{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "updateMode": "lazy",
      "resources": {
        "files": [
          "/index.html",
          "/favicon.ico",
          "/**/*.html",
          "/**/*.css",
          "/**/*.js",
          "/**/*.woff2"
        ]
      }
    },
    {
      "name": "images",
      "installMode": "lazy",
      "updateMode": "lazy",
      "resources": {
        "files": [
          "/assets/**/*",
          "/**/*.png",
          "/**/*.jpg",
          "/**/*.svg",
          "/**/*.gif"
        ]
      }
    }
  ]
}
